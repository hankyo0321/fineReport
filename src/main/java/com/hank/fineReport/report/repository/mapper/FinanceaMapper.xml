<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hank.fineReport.report.repository.mapper.FinanceaMapper">
    <!-- 查詢某個 ID 的記錄 -->
    <select id="findByProId" parameterType="long" resultType="com.hank.fineReport.report.model.Financea">
        SELECT * FROM financea f WHERE f.proId  = #{proId} and f.proDate >=TRUNC(CURRENT_DATE)
    </select>
</mapper>
